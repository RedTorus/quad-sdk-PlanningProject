<sdf version='1.6'>
  <model name='table'>
    <link name='table_top'>
      <pose frame=''>0 0 0.6 0 0 0</pose> <!-- Adjusted z-coordinate to account for new leg length -->
      <inertial>
        <mass>200</mass>
        <inertia>
          <ixx>4.78</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>6.26</iyy>
          <iyz>0</iyz>
          <izz>8.235</izz>
        </inertia>
        <pose frame=''>0 0 0 0 0 0</pose>
      </inertial>
      <gravity>1</gravity>
      <self_collide>0</self_collide>
      <kinematic>0</kinematic>

      <visual name='table_top_visual'>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>4 0.72 0.3</size> <!-- Changed length to 4 -->
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Wood</name>
          </script>
        </material>
        <cast_shadows>1</cast_shadows>
        <transparency>0</transparency>
      </visual>
      <collision name='table_top_collision'>
        <laser_retro>0</laser_retro>
        <pose frame=''>0 0 0 0 0 0</pose>
        <geometry>
          <box>
            <size>4 0.72 0.3</size> <!-- Changed length to 4 -->
          </box>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100</mu>
              <mu2>50</mu2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>

    <!-- Leg 1 -->
    <link name='leg1'>
      <pose frame=''>1.95 0.35 0.3 0 0 0</pose> <!-- Adjusted x-coordinate to account for new table length -->
      <visual name='leg1_visual'>
        <geometry>
          <box>
            <size>0.05 0.05 0.3</size> <!-- Leg length remains 0.3 -->
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Wood</name>
          </script>
        </material>
        <cast_shadows>1</cast_shadows>
        <transparency>0</transparency>
      </visual>
      <collision name='leg1_collision'>
        <geometry>
          <box>
            <size>0.05 0.05 0.3</size> <!-- Leg length remains 0.3 -->
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Leg 2 -->
    <link name='leg2'>
      <pose frame=''>-1.95 0.35 0.3 0 0 0</pose> <!-- Adjusted x-coordinate to account for new table length -->
      <visual name='leg2_visual'>
        <geometry>
          <box>
            <size>0.05 0.05 0.3</size> <!-- Leg length remains 0.3 -->
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Wood</name>
          </script>
        </material>
        <cast_shadows>1</cast_shadows>
        <transparency>0</transparency>
      </visual>
      <collision name='leg2_collision'>
        <geometry>
          <box>
            <size>0.05 0.05 0.3</size> <!-- Leg length remains 0.3 -->
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Leg 3 -->
    <link name='leg3'>
      <pose frame=''>1.95 -0.35 0.3 0 0 0</pose> <!-- Adjusted x-coordinate to account for new table length -->
      <visual name='leg3_visual'>
        <geometry>
          <box>
            <size>0.05 0.05 0.3</size> <!-- Leg length remains 0.3 -->
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Wood</name>
          </script>
        </material>
        <cast_shadows>1</cast_shadows>
        <transparency>0</transparency>
      </visual>
      <collision name='leg3_collision'>
        <geometry>
          <box>
            <size>0.05 0.05 0.3</size> <!-- Leg length remains 0.3 -->
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Leg 4 -->
    <link name='leg4'>
      <pose frame=''>-1.95 -0.35 0.3 0 0 0</pose> <!-- Adjusted x-coordinate to account for new table length -->
      <visual name='leg4_visual'>
        <geometry>
          <box>
            <size>0.05 0.05 0.3</size> <!-- Leg length remains 0.3 -->
          </box>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Wood</name>
          </script>
        </material>
        <cast_shadows>1</cast_shadows>
        <transparency>0</transparency>
      </visual>
      <collision name='leg4_collision'>
        <geometry>
          <box>
            <size>0.05 0.05 0.3</size> <!-- Leg length remains 0.3 -->
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Joints to attach legs to the table top -->
    <joint name='joint_leg1' type='fixed'>
      <parent>table_top</parent>
      <child>leg1</child>
      <pose frame=''>1.95 0.35 0.3 0 0 0</pose> <!-- Adjusted x-coordinate to account for new table length -->
    </joint>
    <joint name='joint_leg2' type='fixed'>
      <parent>table_top</parent>
      <child>leg2</child>
      <pose frame=''>-1.95 0.35 0.3 0 0 0</pose> <!-- Adjusted x-coordinate to account for new table length -->
    </joint>
    <joint name='joint_leg3' type='fixed'>
      <parent>table_top</parent>
      <child>leg3</child>
      <pose frame=''>1.95 -0.35 0.3 0 0 0</pose> <!-- Adjusted x-coordinate to account for new table length -->
    </joint>
    <joint name='joint_leg4' type='fixed'>
      <parent>table_top</parent>
      <child>leg4</child>
      <pose frame=''>-1.95 -0.35 0.3 0 0 0</pose> <!-- Adjusted x-coordinate to account for new table length -->
    </joint>

    <static>0</static>
    <allow_auto_disable>1</allow_auto_disable>
  </model>
</sdf>